<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mycat:rule SYSTEM "rule.dtd">
<mycat:rule xmlns:mycat="http://io.mycat/">
	
	<tableRule name="fixed_uniform_rule">
		<rule>
			<columns>id</columns>
			<algorithm>fixed_uniform</algorithm>
		</rule>
	</tableRule>
	<tableRule name="fixed_nonuniform_rule">
		<rule>
			<columns>id</columns>
			<algorithm>fixed_nonuniform</algorithm>
		</rule>
	</tableRule>
	<tableRule name="fixed_uniform_string_rule">
		<rule>
			<columns>id</columns>
			<algorithm>fixed_uniform_string</algorithm>
		</rule>
	</tableRule>
	<tableRule name="fixed_nonuniform_string_rule">
		<rule>
			<columns>id</columns>
			<algorithm>fixed_nonuniform_string</algorithm>
		</rule>
	</tableRule>
	<tableRule name="enum_integer_rule">
		<rule>
			<columns>id</columns>
			<algorithm>enum_integer</algorithm>
		</rule>
	</tableRule>
	<tableRule name="enum_string_rule">
		<rule>
			<columns>id</columns>
			<algorithm>enum_string</algorithm>
		</rule>
	</tableRule>
	<tableRule name="range_rule">
		<rule>
			<columns>id</columns>
			<algorithm>range</algorithm>
		</rule>
	</tableRule>
	<tableRule name="range_default_rule">
		<rule>
			<columns>id</columns>
			<algorithm>range_string</algorithm>
		</rule>
	</tableRule>
	<tableRule name="date_rule">
		<rule>
			<columns>id</columns>
			<algorithm>date</algorithm>
		</rule>
	</tableRule>
	<tableRule name="date_default_rule">
		<rule>
			<columns>id</columns>
			<algorithm>date_default</algorithm>
		</rule>
	</tableRule>
	<tableRule name="sharding-by-fixed">
		<rule>
			<columns>w_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="sharding-by-fixed1">
		<rule>
			<columns>p_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>

	<tableRule name="mod-long">
		<rule>
			<columns>id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="c_mod-long">
		<rule>
			<columns>c_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="d_mod-long">
		<rule>
			<columns>d_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="h_mod-long">
		<rule>
			<columns>h_c_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="i_mod-long">
		<rule>
			<columns>i_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="n_mod-long">
		<rule>
			<columns>no_o_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="ol_mod-long">
		<rule>
			<columns>ol_number</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="o_mod-long">
		<rule>
			<columns>o_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="s_mod-long">
		<rule>
			<columns>s_i_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="w_mod-long">
		<rule>
			<columns>w_id</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>
	<tableRule name="f_mod-long">
		<rule>
			<columns>entryUuid</columns>
			<algorithm>sharding</algorithm>
		</rule>
	</tableRule>

	<function name="fixed_uniform" class="io.mycat.route.function.PartitionByLong">
		<property name="partitionCount">4</property>
		<property name="partitionLength">256</property>
	</function>		
	<function name="fixed_nonuniform" class="io.mycat.route.function.PartitionByLong">
		<property name="partitionCount">2,1</property>
		<property name="partitionLength">256,512</property>
	</function>
	<function name="fixed_uniform_string" class="io.mycat.route.function.PartitionByString">
		<property name="partitionCount">4</property>
		<property name="partitionLength">256</property>
		<property name="hashSlice">0:2</property>
	</function>		
	<function name="fixed_nonuniform_string" class="io.mycat.route.function.PartitionByString">
		<property name="partitionCount">2,1</property>
		<property name="partitionLength">256,512</property>
		<property name="hashSlice">0:2</property>
	</function>	
	<function name="enum_integer" class="io.mycat.route.function.PartitionByFileMap">
		<property name="mapFile">enum-integer.txt</property>
		<property name="type">0</property>
		<property name="defaultNode">0</property>
	</function>	
	<function name="enum_string" class="io.mycat.route.function.PartitionByFileMap">
		<property name="mapFile">enum-string.txt</property>
		<property name="type">1</property>
		<property name="defaultNode">0</property>
	</function>	
	<function name="range" class="io.mycat.route.function.AutoPartitionByLong">
		<property name="mapFile">range.txt</property>
		<!--<property name="defaultNode">0</property>-->
	</function>	
	<function name="range_string" class="io.mycat.route.function.AutoPartitionByLong">
		<property name="mapFile">range_default.txt</property>
		<property name="defaultNode">3</property>
	</function>	
	<function name="date" class="io.mycat.route.function.PartitionByDate">
		<property name="dateFormat">yyyy-MM-dd</property>
		<property name="sBeginDate">2016-12-01</property>
		<property name="sEndDate">2017-01-9</property>
		<property name="sPartionDay">10</property>
		<!--<property name="defaultNode">0</property>-->
	</function>	
	<function name="date_default" class="io.mycat.route.function.PartitionByDate">
		<property name="dateFormat">yyyy-MM-dd</property>
		<property name="sBeginDate">2016-12-01</property>
		<property name="sEndDate">2017-01-9</property>
		<property name="sPartionDay">10</property>
		<property name="defaultNode">0</property>
	</function>	
	<function name="sharding" class="io.mycat.route.function.PartitionByLong">
		<property name="partitionCount">4</property>
		<property name="partitionLength">1</property>
	</function>	
</mycat:rule>	
